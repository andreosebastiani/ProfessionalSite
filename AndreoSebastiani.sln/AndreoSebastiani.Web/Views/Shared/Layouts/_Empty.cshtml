<!DOCTYPE html>
<html>
<head>
    <title>
        @ViewBag.Title
    </title>

    <meta name="viewport" content="width=device-width, initial-scale=1" />

    @Styles.Render("~/Content/commoncss")

    <link rel="icon" href="@Url.Content("~/Content/Images/favicon.ico")" />

    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">

    @RenderSection("pagecss", false)

</head>

<body class="site-wrapper">

    @RenderBody()


    @Html.Action("Footer", "NavBar")



    @Scripts.Render("~/bundles/commonjavascript")

    @RenderSection("pagescripts", false)

    @Html.Partial("~/Views/Shared/Layouts/_GoogleAnalytics.cshtml")

    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>


</body>


</html>
